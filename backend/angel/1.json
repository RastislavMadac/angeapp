							"var template = `",
							"<canvas id=\"expeditionChart\" height=\"100\"></canvas>",
							"<script src=\"https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.5.0/Chart.min.js\"></script>",
							"<script>",
							"    var ctx = document.getElementById(\"expeditionChart\");",
							"    var expeditionChart = new Chart(ctx, {",
							"        type: \"bar\",",
							"        data: {",
							"            labels: [],",
							"            datasets: [",
							"                {",
							"                    label: \"Items Count\",",
							"                    backgroundColor: \"rgba(54, 162, 235, 0.7)\",",
							"                    borderColor: \"rgba(54, 162, 235, 1)\",",
							"                    borderWidth: 1,",
							"                    data: []",
							"                },",
							"                {",
							"                    label: \"Prepared Items Count\",",
							"                    backgroundColor: \"rgba(255, 99, 132, 0.7)\",",
							"                    borderColor: \"rgba(255, 99, 132, 1)\",",
							"                    borderWidth: 1,",
							"                    data: []",
							"                }",
							"            ]",
							"        },",
							"        options: {",
							"            legend: { display: true },",
							"            title: {",
							"                display: true,",
							"                text: \"Expedition Items vs Prepared Items\"",
							"            },",
							"            scales: {",
							"                xAxes: [{",
							"                    display: true,",
							"                    scaleLabel: {",
							"                        display: true,",
							"                        labelString: \"Order Number\"",
							"                    }",
							"                }],",
							"                yAxes: [{",
							"                    display: true,",
							"                    scaleLabel: {",
							"                        display: true,",
							"                        labelString: \"Count\"",
							"                    },",
							"                    ticks: {",
							"                        beginAtZero: true",
							"                    }",
							"                }]",
							"            }",
							"        }",
							"    });",
							"",
							"    pm.getData(function (err, value) {",
							"        expeditionChart.data.labels = value.labels;",
							"        expeditionChart.data.datasets[0].data = value.itemsCounts;",
							"        expeditionChart.data.datasets[1].data = value.preparedItemsCounts;",
							"        expeditionChart.update();",
							"    });",
							"</script>",
							"`;",
							"",
							"function constructVisualizerPayload() {",
							"    var response = pm.response.json();",
							"    var labels = response.map(function(expedition) {",
							"        return expedition.order_number;",
							"    });",
							"    var itemsCounts = response.map(function(expedition) {",
							"        return expedition.items ? expedition.items.length : 0;",
							"    });",
							"    var preparedItemsCounts = response.map(function(expedition) {",
							"        return expedition.prepared_items ? expedition.prepared_items.length : 0;",
							"    });",
							"    ",
							"    return {",
							"        labels: labels,",
							"        itemsCounts: itemsCounts,",
							"        preparedItemsCounts: preparedItemsCounts",
							"    };",
							"}",
							"",
							"pm.visualizer.set(template, constructVisualizerPayload());"
						],
						"type": "text/javascript",
